<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph.xml"
    app:startDestination="@id/profileFragment">
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.itis.summerproject23.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_registrationFragment"
            app:destination="@id/registrationFragment" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="com.itis.summerproject23.RegistrationFragment"
        android:label="RegistrationFragment" >
        <action
            android:id="@+id/action_registrationFragment_to_userAcoountFragment"
            app:destination="@id/userAcoountFragment" />
    </fragment>
    <fragment
        android:id="@+id/userAcoountFragment"
        android:name="com.itis.summerproject23.UserAccountFragment"
        android:label="UserAcoountFragment" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.itis.summerproject23.SearchFragment"
        android:label="SearchFragment" >
        <action

            android:id="@+id/action_searchFragment_to_recipePageFragment"

            app:destination="@id/recipePageFragment" />
    </fragment>
    <fragment
        android:id="@+id/addFragment"
        android:name="com.itis.summerproject23.AddFragment"
        android:label="fragment_add"
        tools:layout="@layout/fragment_add" />
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.itis.summerproject23.FavoriteFragment"
        android:label="fragment_favorite"
        tools:layout="@layout/fragment_favorite" >
        <action
            android:id="@+id/action_favoriteFragment_to_recipePageFragment"
            app:destination="@id/recipePageFragment" />
    </fragment>
    <fragment
        android:id="@+id/recipePageFragment"
        android:name="com.itis.summerproject23.RecipePageFragment"
        android:label="fragment_recipe_page"
        tools:layout="@layout/fragment_recipe_page" >
        <action
            android:id="@+id/action_recipePageFragment_to_searchFragment"
            app:destination="@id/searchFragment" />
    </fragment>
</navigation>